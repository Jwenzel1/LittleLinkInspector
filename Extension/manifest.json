{
  "manifest_version": 2,
  "name": "Bit.ly Link Inspecter",
  "version": "0.1",
  "permissions": ["https://swapi.co/"],
  "web_accessible_resources": ["images/DontPanic.jpg"],


  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/styles.css"],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ]

}
